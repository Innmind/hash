{
    "name": "innmind/hash",
    "type": "library",
    "description": "Files incremental hash",
    "keywords": ["hash", "file"],
    "homepage": "http://github.com/innmind/hash",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/innmind/hash/issues"
    },
    "require": {
        "php": "~8.2",
        "innmind/immutable": "~4.5",
        "innmind/filesystem": "^5.2|~6.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Hash\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Hash\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~10.2",
        "vimeo/psalm": "~5.11",
        "innmind/black-box": "~5.5",
        "innmind/coding-standard": "~2.0"
    }
